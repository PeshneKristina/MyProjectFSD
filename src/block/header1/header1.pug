include \..\..\block\full-logo\full-logo
include \..\..\block\header-panel\header-panel
include \..\..\block\button-border\button-border
include \..\..\block\button\button

mixin header1(items,numbers,enter, register)
    .header1
        .header1__logo
            +full-logo()
        .header1__panel
            +header-panel(items,numbers)
        .header1__login
            +button-border(enter)
        .header1__register
            +button(register)